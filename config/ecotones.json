// Configuration file for Ecotones. This file uses the JSON format, with comments delimited by two slashes (//).
// Comments are only supported on newlines.
{
    // Controls the size of biomes. 0 = tiny, 1 = small, 2 = medium (default), 3 = large, 4 = humungous.
    "biomeSize": 2,

    // Client settings.
    "client": 
    {
        // Ecotones uses an enhanced skybox when in an ecotones world. You can disable that here.
        "useEcotonesSky": true
    }
}
